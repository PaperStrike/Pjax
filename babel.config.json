{
  "presets": [
    ["@babel/preset-typescript", {
      "allowDeclareFields": true
    }],
    ["@babel/preset-env", {
      "bugfixes": true
    }]
  ],
  "plugins": [
    ["@babel/transform-runtime", {
      "version": "^7.15.0"
    }],
    ["polyfill-corejs3", {
      "method": "usage-pure",
      "exclude": [
        "web.url",
        "es.array.reduce",
        "es.array.sort",
        "es.promise"
      ]
    }]
  ]
}
