{
  "assumptions": {
    "setPublicClassFields": true,
    "noDocumentAll": true
  },
  "presets": [
    ["@babel/preset-env", {
      "bugfixes": true
    }],
    ["@babel/preset-typescript", {
      "allowDeclareFields": true
    }]
  ],
  "plugins": [
    ["polyfill-corejs3", {
      "method": "usage-pure",
      "exclude": [
        "web.url",
        "web.url-search-params",
        "es.array.reduce",
        "es.array.sort",
        "es.promise"
      ]
    }]
  ]
}
