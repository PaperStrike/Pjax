{
  "name": "@sliphua/pjax",
  "version": "2.0.2",
  "description": "Easily enable fast AJAX navigation (using Fetch + pushState)",
  "keywords": [
    "pjax",
    "pushstate",
    "ajax",
    "fetch",
    "navigation",
    "transition"
  ],
  "bugs": "https://github.com/PaperStrike/Pjax/issues",
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "tsconfig.json",
    "*.config.*",
    ".eslintrc.*",
    "CHANGELOG.*"
  ],
  "exports": {
    "browser": {
      "development": {
        "import": "./dist/pjax.esm.js",
        "script": "./dist/pjax.js"
      },
      "production": {
        "import": "./dist/pjax.esm.min.js",
        "script": "./dist/pjax.min.js"
      }
    }
  },
  "repository": "github:PaperStrike/Pjax",
  "browserslist": [
    "edge >= 79",
    "firefox >= 60",
    "chrome >= 66",
    "opera >= 53",
    "safari >= 12.2"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.27.0",
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jest": "^27.0.0",
    "@types/web": "^0.0.24",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.29.0",
    "babel-plugin-polyfill-corejs3": "^0.2.0",
    "cross-fetch": "^3.1.4",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^13.0.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.0.1",
    "jest-environment-jsdom": "^27.0.1",
    "nock": "^13.0.11",
    "rollup": "^2.56.3",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "^14.15.0 || >=15.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "test": "jest",
    "coverage": "jest --coverage",
    "prepare": "npm run build"
  }
}
